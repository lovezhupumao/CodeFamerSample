<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zpm.codefarmer.sample">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CircleButtonActivity" />
        <activity android:name=".CircleImageActivity" />
        <activity android:name=".CircularFloatingActionMenuActivity" />
        <activity android:name=".FlatButtonActivity" />
        <activity android:name=".FloatActionButtonActivity" />
        <activity android:name=".HtextViewActivity" />
        <activity android:name=".PassordEditviewActivity" />
        <activity android:name=".RoundedImageviewActivity" />
        <activity android:name=".SmoothCheckboxActivity" />
        <activity android:name=".SwitchbuttonActivity" />
        <activity android:name=".NumberProgressBarActivity" />
        <activity android:name=".CircleProgressBarActivity" />
        <activity android:name=".NiceSpinnerActivity" />
        <activity android:name=".DialogplusActivity" />
        <activity android:name=".BetterSpinnerActivity" />
        <activity android:name=".retrofit.RetrofitActivity"></activity>
    </application>

</manifest>